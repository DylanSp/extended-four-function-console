{
  "name": "wheel-lang",
  "version": "0.3.0",
  "description": "A small toy imperative language for demonstrating and practicing language design/implementation.",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "lint": "eslint --max-warnings 0 src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts"
  },
  "keywords": [],
  "author": "Dylan Sprague",
  "repository": "github:DylanSp/wheel-lang",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/readline-sync": "^1.4.3",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": {
          "plugins": [
            "@babel/plugin-proposal-optional-chaining"
          ]
        }
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "fp-ts": "^2.10.5",
    "monocle-ts": "^2.3.10",
    "newtype-ts": "^0.3.4",
    "readline-sync": "^1.4.10",
    "yargs": "^17.0.1"
  }
}
